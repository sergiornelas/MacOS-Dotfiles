{
  "name": "Vimium C",
  "autoDarkMode": 1,
  "keyLayout": 2,
  "keyMappings": [
    "map w removeTab",
    "map <c-g> visitPreviousTab",
    "map ' Marks.activate",
    "map a focusInput",
    "map e LinkHints.activateOpenInNewTab",
    "map F visitPreviousTab",
    "map q LinkHints.activateWithQueue",
    "map s LinkHints.activateSelect",
    "map ga LinkHints.activateEdit",
    "map gc LinkHints.click",
    "map gh LinkHints.activateHover",
    "map gl LinkHints.activateSearchLinkText",
    "map gx LinkHints.activateFocus",
    "map gy autoOpen",
    "map zi zoomIn",
    "map zo zoomOut",
    "map zr zoomReset",
    "map J goBack",
    "map K goForward",
    "map S goBack",
    "map D goForward",
    "map H moveTabLeft",
    "map L moveTabRight",
    "map E previousTab",
    "map R nextTab"
