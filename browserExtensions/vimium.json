{
  "name": "Vimium C",
  "autoDarkMode": 1,
  "keyLayout": 2,
  "keyMappings": [
    "map ' Marks.activate",
    "map e LinkHints.activateOpenInNewTab",
    "map F LinkHints.activateOpenInNewForegroundTab",
    "map q LinkHints.activateWithQueue",
    "map s LinkHints.activateSelect",
    "map <c-space> visitPreviousTab",
    "map ga LinkHints.activateEdit",
    "map gc LinkHints.click",
    "map gh LinkHints.activateHover",
    "map gl LinkHints.activateSearchLinkText",
    "map gx LinkHints.activateFocus",
    "map gy autoOpen",
    "map zi zoomIn",
    "map zo zoomOut",
    "map zr zoomReset"
  ],
  "linkHintCharacters": "jiofewapklqs",
  "smoothScroll": false,
  "userDefinedCss": ["/* #omni */", "#bar { background: #4F0A0A; }", ""],
  "vimSync": true
}
