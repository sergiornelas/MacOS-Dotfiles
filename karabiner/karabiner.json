        "rules": [
          {
            "description": "Right_command to Right_Control if held, Esc if it's tapped",
            "manipulators": [
              {
                "from": {
                  "key_code": "right_command",
                  "modifiers": {
                    "optional": ["any"]
                  }
                },
                "to": [
                  {
                    "key_code": "right_control",
                    "lazy": true
                  }
                ],
                "to_if_alone": [
                  {
                    "key_code": "escape"
                  }
                ],
                "type": "basic"
              }
            ]
          },
          {
            "description": "Caps to HYPER when held, Recent window when tap (yabai)",
