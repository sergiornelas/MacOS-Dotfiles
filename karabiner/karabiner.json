              {
                "from": {
                  "key_code": "caps_lock",
                  "modifiers": {
                    "optional": ["any"]
                  }
                },
                "to": [
                  {
                    "key_code": "left_shift",
                    "modifiers": ["left_command", "left_control", "left_option"]
                  }
                ],
                "to_if_alone": [
                  {
                    "key_code": "up_arrow",
                    "modifiers": [
                      "left_shift",
                      "left_command",
                      "left_control",
                      "left_option"
                    ]
                  }
                ],
                "type": "basic"
              }
            ]
